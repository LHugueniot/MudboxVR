include buildconfig


libMudboxVR.so : MudboxVR.o
		$(CC) -o libMudboxVR.so MudboxVR.o $(LINKFLAGS) #-lMapExtractor -Wl
MudboxVR.o : MudboxVR.cpp

clean :
		rm libMudboxVR.so *.o

install :
		cp libMudboxVR.so ../plugins